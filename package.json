{
  "name": "@codeandmoney/dorjo",
  "version": "0.0.0-dev.4",
  "description": "Zero-Abstraction Postgres for TypeScript",
  "keywords": [
    "postgres",
    "typescript",
    "orm",
    "sql"
  ],
  "license": "MIT",
  "exports": {
    "./db": "./src/db/exports.ts",
    "./generate": "./src/generate/exports.ts"
  },
  "bin": {
    "dorjo": "dist/generate/cli.js"
  },
  "scripts": {
    "build": "bunx tsc",
    "postinstall": "bun --eval \"console.warn('>>> \\u001b[1;33mDorjo was installed or upgraded. Please (re-)run \\u001b[1;32mnpx dorjo\\u001b[1;33m.\\u001b[0m <<<\\u000a')\"",
    "version": "pm run build",
    "postversion": "git push && git push --tags && npm publish"
  },
  "dependencies": {
    "es-toolkit": "^1.43.0",
    "json-custom-numbers": "^3.1.1"
  },
  "devDependencies": {
    "@types/bun": "^1.3.3",
    "@types/pg": "^8.15.6",
    "pg": "^8.16.3",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@types/pg": ">=7.14.3",
    "pg": ">=7.18.2",
    "typescript": ">=4.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
